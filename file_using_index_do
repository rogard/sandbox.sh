_do()
{
  newdir="$1/$3"
  old_path="$1/$2"
  [[ -d "$newdir" ]] || mkdir "$newdir"
  cp "$old_path" "$newdir"
  file_history="$newdir/file_history"
  touch "$file_history"
  printf "%s\t" 'Old path:' "$old_path" >> "$file_history"
  echo "" >> "$file_history"
  next=$(stat "$old_path" | grep 'Modify:' | cut -c 9-)
  printf "%s\t" 'Modify:' "$next"  >> "$file_history"
  mv "$old_path" "$old_path.trash"
}
